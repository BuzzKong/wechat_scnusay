<!--pages/enroll/enroll.wxml-->
<form bindsubmit="regist">
	<view>
		<view class="second">
			<input id="userphone" placeholder="请输入手机号" placeholder-class="plas" class="inputs" type="number" bindinput="phoneInput" name="userphone"></input>
		</view>
		<view class="second2">
			<input id="scnu_number" placeholder="请输入学号" placeholder-class="plas" class="inputs" type="number" bindinput="scnuNumberInput" name="scnu_number"></input>
		</view>
		<view>
			<button class="click" type="primary" form-type='submit' bindtap="regist">确认信息</button>
		</view>
		<view class="cha" bindtap="enroll"><text class="no" style="color: steelblue;">已有账号，点我登录</text></view>
	</view>
</form>
