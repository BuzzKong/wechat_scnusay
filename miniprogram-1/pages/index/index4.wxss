page{
    height: 100%;
}
  /* 头像 */
  .userinfo {
    display: flex;
    flex-direction: row;
    align-items: center;
    color: #aaa;
    margin-top: 30%;
  }
  
  .userinfo-avatar {
    overflow: hidden;
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
  }
.loginbutton{
  margin-right: 10px !important;
  width: 80% ;
}
.header {
    margin: 90rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
  }
  .header image {
    width: 200rpx;
    height: 200rpx;
  }
  .content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
  }
  .content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
  }
  .bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
  }
  .container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    background-image: url(https://s1.328888.xyz/2022/05/15/qwCrZ.png);
  } 
  .container2 {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
  } 
  .welcometext{
      height: 200rpx;
      color: white;
      font-size: 25px;
      font-weight: 800;
      
  }
  .welcomelogo{
      width: 200rpx;
      height: 200rpx;
      margin-left: 30rpx;
  }
  .headwelcome{
      display: flex;
      margin-top: 150rpx;
      width: 90%;
      justify-content: space-evenly;
  }